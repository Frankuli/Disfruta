<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>INGRESA</title>
    <link rel="icon" type="image/png" href="img/0.png">
    <link rel="stylesheet" href="css/normalize.css">
    <link rel="stylesheet" href="css/bootstrap.min.css">
    <link rel="stylesheet" href="css/sesion.css">
  </head>
  <body>
    <div class="form">
        <div id="reg">
          <form ref="uploadForm" id="uploadForm" method="post" name="subirFruta" encType="multipart/form-data" action="frutas">
            <input type="text" name="name" value="" placeholder="Nombre">
            <input type="number" name="price" value="" placeholder="precio">
            <input type="file" name="img" accept="image/*">
            <input type="number" name="cantidad" value="" placeholder="cantidad">
            <input type="checkbox" name="promo" value="1" style="padding:4px;height: 10px;"> 
            <select form="subirFruta" name="tipo">
              <option value="fruta">fruta</option>
              <option value="verdura">verdura</option>
            </select>
            <input type="submit" value="subir">
          </form>
        </div>
    </div>
    <script src='http://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.3/jquery.min.js'></script>
    <script src="js/sesion.js"></script>
  </body>
</html>
